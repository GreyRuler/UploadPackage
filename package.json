{
	"name": "chat",
	"version": "1.0.0",
	"scripts": {
		"start": "webpack-dev-server --mode development --config webpack.frontend.ts",
		"build": "webpack --mode production --config webpack.frontend.ts",
		"start:server": "yarn npm-run-all --parallel watch:server watch:build",
		"watch:build": "webpack --watch --config webpack.server.ts",
		"watch:server": "nodemon \"./dist/backend/server.js\" --watch \"./dist/backend\"",
		"serve": "node dist/backend/server.js",
		"build:server": "webpack --mode production --config webpack.server.ts",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"test": "jest",
		"deploy": "gh-pages -d dist/frontend"
	},
	"repository": {
		"type": "git"
	},
	"author": "youngSniff",
	"license": "MIT",
	"devDependencies": {
		"@jest/globals": "^29.0.3",
		"@types/bootstrap": "^5.2.6",
		"@types/jest": "^29.1.2",
		"@types/koa": "^2.13.5",
		"@types/koa__cors": "^3.3.0",
		"@types/koa__router": "^12.0.0",
		"@types/node": "^18.13.0",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"css-loader": "^6.7.1",
		"file-loader": "^6.2.0",
		"gh-pages": "^4.0.0",
		"html-loader": "^4.1.0",
		"jest": "^29.0.3",
		"koa-static": "^5.0.0",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^19.4.0",
		"svg-inline-loader": "^0.8.2",
		"ts-jest": "^29.0.3",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"url-loader": "^4.1.1",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.0"
	},
	"dependencies": {
		"@koa/cors": "^4.0.0",
		"@koa/router": "^12.0.0",
		"bootstrap": "^5.2.3",
		"core-js": "^3.25.2",
		"html-webpack-plugin": "^5.5.0",
		"koa": "^2.14.1",
		"koa-body": "^6.0.1",
		"mini-css-extract-plugin": "^2.6.1",
		"webpack": "^5.75.0"
	}
}
